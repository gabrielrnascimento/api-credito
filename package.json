{
  "name": "api-credito",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "ts-node-dev --respawn src/main/server.ts",
    "start": "node dist/src/main/server.js",
    "build": "rm -rf dist/ && tsc",
    "build:start": "npm run build && npm start",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:unit": "npm test -- -c jest-unit-config.js",
    "test:integration": "npm test -- -c jest-integration-config.js",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:vwatch": "npm run test:verbose -- --watch",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coverage": "npm test -- --coverage",
    "prepare": "husky install",
    "up": "docker compose up -d",
    "down": "docker compose down",
    "seed": "seed -u 'mongodb://localhost:27017/culttivo' --drop-database ./data/seeds/"
  },
  "author": "Gabriel Nascimento",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^4.1.7",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.4.5",
    "@types/supertest": "^2.0.12",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "mongo-seeding-cli": "^3.7.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^5.7.0"
  }
}
